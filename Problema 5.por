programa {
  funcao inicio() {
    //Declaração de Variáveis
      real altura,peso_ideal

    //Declaração de Constantes
      const real k =72.7  
      const inteiro b=58

    //Entrada de Dados
      escreva ("Informe a altura:")
      leia(altura)

    //Processamento
      peso_ideal = (k*altura)-b

    //Saída de Dados
    escreva("O peso ideal é:",peso_ideal)   

  }
}
