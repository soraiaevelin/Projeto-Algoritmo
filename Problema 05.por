programa {
  funcao inicio() {
    //Declaraçõ de variáveis
      real altura,peso_ideal

    //Declaração de constante
      const real k=72.7
      const inteiro b=58

    //Entrada de dados
      escreva("Informe a altura")
      leia(altura)

    //Processamento
      peso_ideal= k*altura-b

    //Saída de dados
      escreva("O peso ideal e",peso_ideal)    


  }
}
