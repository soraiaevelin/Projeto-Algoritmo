programa {
  funcao inicio() {
    //Declaração de Variáveis
      real fahrenheit,celsius
      inteiro fahrenheit,celsius

    //Entrada de Dados
      escreva("Informe a temperatura em fahrenheit:")
      leia(fahrenheit)  

    //Processamento
      celsius = ((fahrenheit-32)*5)/9

    //Saída de Dados
      escreva("A temperatura em celsius é:",celsius)    
  }
}