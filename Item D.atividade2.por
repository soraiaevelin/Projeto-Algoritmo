programa {
  funcao inicio() {
    //Declaração de Variáveis
      real celsius,fahrenheit
      inteiro celsius,fahrenheit

    //Entrada de Dados
      escreva("Informe a temperatura em Celsius:")
      leia(celsius)  

    //Processamento
      fahrenheit = (celsius * 9/5)+32

    //Saída de Dados
      escreva("A temperatura em fahrenheit é:",fahrenheit)    
  }
}

