programa {
  funcao inicio() {
    //Declaração de Variáveis
      real total,ganhador1,ganhador2,ganhador3

    //Processamento
      total =780.000

      ganhador1=total*0.46
      ganhador2=total*0.32
      ganhador3 =total-ganhador1-ganhador2

    //Saída de Dados

      escreva("O primeiro ganhador receberá:",ganhador1)
      escreva("O segundo ganhador receberá:",ganhador2)
      escreva("O terceiro ganhador receberá:",ganhador3)
  
  }
}
